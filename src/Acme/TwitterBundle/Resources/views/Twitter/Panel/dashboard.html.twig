{% extends '::index.html.twig' %}
{% block title %}
    {{ sdata.panel.title.dash }}
{% endblock %}
{% block content %}
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try {
                ace.settings.check('breadcrumbs', 'fixed')
            } catch (e) {
            }
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="#">{{ sdata.panel.menu.dash }} </a>
            </li>
            <li class="active">{{ sdata.panel.menu.dash }} </li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
                <span class="input-icon">
                    <input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
                    <i class="icon-search nav-search-icon"></i>
                </span>
            </form>
        </div><!-- #nav-search -->
    </div>

    <div class="page-content">
        <div class="page-header">
            <h1>
                {{ sdata.panel.menu.dash }}
                <small>
                    <i class="icon-double-angle-right"></i>
                    {{ sdata.panel.head.genLookGrap }}
                </small>
            </h1>
        </div><!-- /.page-header -->
        <div class="row">
            <div class="alert alert-block alert-info">
                <button type="button" class="close" data-dismiss="alert">
                    <i class="icon-remove"></i>
                </button>

                <i class="icon-info fa-caret-right blue"></i>

                {{ sdata.panel.mess.staRefrPage }}
            </div>


            <div class="col-sm-12 infobox-container">
                <h3 class="header smaller lighter blue"> {{ sdata.panel.head.genLook }} </h3>

                <div class="infobox infobox-blue  ">
                    <div class="infobox-icon">
                        <i class="icon-twitter"></i>
                    </div>

                    <div class="infobox-data">
                        <span class="infobox-data-number">{{tweets | length }}</span>
                        <div class="infobox-content">T. Tweet</div>
                    </div>

                    <div class="badge badge-success">
                        +32%
                        <i class="icon-arrow-up"></i>
                    </div>
                </div>

                <div class="infobox infobox-blue  ">
                    <div class="infobox-icon">
                        <i class="icon-twitter"></i>
                    </div>

                    <div class="infobox-data">
                        <span class="infobox-data-number">{{ packets | length }}</span>
